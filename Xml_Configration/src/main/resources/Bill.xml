<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="credit" class="com.org.BillCollector.CreditCard"/>
	<bean id="Debit" class="com.org.BillCollector.DebitCard"/>
	<bean id="upi" class="com.org.BillCollector.UPI" scope="prototype"/>
	<bean id="bill" class="com.org.BillCollector.BillCollector">
	<property name="Payment" ref="upi"></property>
	 </bean>
    
	
	
</beans>